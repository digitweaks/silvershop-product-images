{
    "name": "digitweaks/silvershop-product-images",
    "type": "silverstripe-vendormodule",
    "description": "A SilverShop module to add multiple product images",
    "require": {
        "silvershop/core": "^3 || ^4",
        "bummzack/sortablefile": "^1.5 || ^2", 
        "silverstripe/vendor-plugin": "^2"
    },
    "autoload": {
        "psr-4": {
            "DigiTweaks\\SilverShopProductImages\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.*-dev"
        }
    },
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "silverstripe/recipe-plugin": true
        }
    }
}
